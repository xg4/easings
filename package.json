{
  "name": "@xg4/easings",
  "version": "0.0.3",
  "description": "easings",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "typings": "dist/types/index.d.ts",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "lint": "tslint src/**/*.{ts,tsx} ",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c",
    "build": "npm run build:types && npm run build:js",
    "release": "npm run build && git commit -am \"v\"$npm_package_version && git tag \"v\"$npm_package_version && git push && git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint",
      "git add"
    ]
  },
  "keywords": [
    "easings"
  ],
  "author": "xg4 <xingor4@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "rollup": "^1.4.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.4",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xg4/easings.git"
  },
  "bugs": {
    "url": "https://github.com/xg4/easings/issues"
  },
  "homepage": "https://github.com/xg4/easings#readme"
}
